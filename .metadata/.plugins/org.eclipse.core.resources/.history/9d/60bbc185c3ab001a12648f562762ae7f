package com.wityo.dynamicprogramming.bellmanford;

class Edge {
	public Edge(int source, int destination, int weight) {
		super();
		this.source = source;
		this.destination = destination;
		this.weight = weight;
	}

	int source;
	int destination;
	int weight;

}

